<table width="70%" cellpadding="10" cellspacing="0">
<tr>
<td>
<center>
    <script>
      jmolInitialize("../jmol"); // REQUIRED
        jmolHtml("<font size=12><b>Examples of Cyclodextrins<b></font>")
	jmolBr();
	jmolBr();
	jmolBr();
      jmolApplet(300, "load ../jmol/grn1_cd.pdb");
	jmolBr();
        jmolHtml("<b>Heptakis(2,3-di-O-methyl-6-O-sulfopropyl)-beta-cyclodextrin (Green)</b>")
	jmolBr();
    </script>
</center>
</td>
</tr>
<tr>
<td>
<center>
    <script>
      jmolApplet(300, "load ../jmol/grn2_cd.pdb");
	jmolBr();
        jmolHtml("<b>Hexakis(2,3-di-O-methyl-6-O-sulfopropyl)-alpha-cyclodextrin (Green)</b>")
	jmolBr();
    </script>
</center>
</td>
</tr>
<td>
<center>
    <script>
      jmolApplet(300, "load ../jmol/vigh2_cd.pdb");
	jmolBr();
        jmolHtml("<b>Heptakis-(2,3-dimethyl-6-sulfato)-beta-cyclodextrin (Vigh)</b>")
	jmolBr();
    </script>
</center>
</td>
<tr>
<td>
<center>
<form> <!-- form tags must enclose button/checkbox/radio/menu controls -->
      <script>
	jmolBr();
        jmolSetAppletColor("skyblue"); // if you don't want black

	// 200x200 + file from another directory
        jmolApplet(250, "load ../jmol/beta_cd.pdb");
	jmolBr();
        jmolHtml("<b>An unsubstituted cyclodextrin</b>")
	jmolBr();
	jmolBr();
	// a radio group
	jmolHtml("atoms ");
	jmolRadioGroup([
          ["spacefill off",  "off"],
	  ["spacefill 20%",  "20%", "checked"],
	  ["spacefill 100%", "100%"]
	]);
	jmolBr();
	jmolBr();
	// a button
	jmolButton("reset", "Reset to original orientation");
	jmolBr();
	jmolBr();
	// a checkbox
	jmolCheckbox("spin on", "spin off", "spin");
	jmolBr();
	jmolBr();
	// a link
	jmolLink("move 360 0 0 0 0 0 0 0 2", "Rotate once about the x axis");
	jmolBr();
	jmolBr();
	// a menu
	jmolMenu([
	"background white",
	["background skyblue", null, "selected"],
	"background yellow",
	"background salmon",
	"background palegreen",
	"background black"
	]);
      </script>
    </form>
</center>
</td>
</tr>
